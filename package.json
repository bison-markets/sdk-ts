{
  "name": "@bison-markets/sdk-ts",
  "version": "0.1.4",
  "description": "Bison Markets SDK (TypeScript)",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/bison-markets/sdk-ts#readme",
  "bugs": {
    "url": "https://github.com/bison-markets/sdk-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bison-markets/sdk-ts.git"
  },
  "license": "AGPL-3.0-only",
  "author": "",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "generate:openapi": "openapi-typescript ${OPENAPI_URL:-http://localhost:8787/openapi} --output ./src/openapi/generated/schema.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "openapi-typescript": "^7.4.4",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "openapi-fetch": "^0.15.0",
    "viem": "^2.38.6"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
